{
    "name": "folklore/laravel-panneau",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "http://github.com/folkloreinc",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@folklore.email",
            "homepage": "https://folkloreinc.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@folklore.email",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "guzzlehttp/guzzle": "5.3|~6.0",
        "spatie/eloquent-sortable": "^2.0|^3.0",
        "folklore/eloquent-json-schema": "1.1.x-dev",
        "folklore/laravel-mediatheque": "1.1.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/database": "3.5.*|3.6.*|3.7.*",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "mockery/mockery": "^1.0.0",
        "phpunit/phpunit": "~7.0",
        "php-coveralls/php-coveralls": "^2.1",
        "doctrine/dbal": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Panneau\\": "src/Panneau/"
        },
        "files": [
            "src/Panneau/helpers.php"
        ],
        "classmap": [
            "scripts/PanneauJsInstaller.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Panneau\\Tests\\": "tests"
        },
        "classmap": [
            "tests/fixture/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Panneau\\PanneauServiceProvider"
            ],
            "aliases": {
                "Panneau": "Panneau\\Support\\Facades\\Panneau"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
